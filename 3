programa
{
	
	funcao inicio()
	{
		real RSdia = 60
		real RSkm = 0.15
		inteiro dia 
		inteiro km
		real preco

		escreva("Me diga quantos quilometros voce rodou: ")
		leia(km)

		escreva("Me diga quantos km esse carro rodou: ")
		leia(dia)

		preco = km * RSkm + dia * RSdia

		escreva(" o preco final e de R$" , preco)
	}
}
